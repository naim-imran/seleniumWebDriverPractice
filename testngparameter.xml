<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<!--<suite name="Parallel Suite" parallel="tests" thread-count="5">-->
	<suite name="WebDriverTest" parallel="classes" thread-count="5">
	<listeners>
		<listener class-name="webDriverPractice.listeners.RerunFailedTestTransformer"></listener>
		<listener class-name="webDriverPractice.listeners.Listeners"></listener>
	</listeners>
	<parameter name="url" value="ebay"></parameter>


	<test name="Test">
		<classes>
			<class name="webDriverPractice.test.R0011BrokenLinkVerification">
				<methods>
					<exclude name="testBrokenLink()"></exclude>
				</methods>
			</class>
			<class name="webDriverPractice.test.R0008TakeScreenShots"></class>
		</classes>
	</test>

</suite>

